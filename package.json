{
  "name": "@sab/cdn",
  "private": true,
  "description": "A CDN service for frontend artifacts within the SAB infrastructure",
  "scripts": {
    "clean": "git clean -e '!/.env' -e '!/.env.docker' -fdX .",
    "lint": "eslint .",
    "test": "jest",
    "build": "rollup --config rollup.config.cjs",
    "build:watch": "rollup --config rollup.config.cjs --watch",
    "serve": "nodemon -r dotenv/config -w dist/server.js dist/server.js",
    "start": "run-p build:watch serve",
    "start:docker": "docker compose up --build"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "cors": "2.8.5",
    "etag": "1.8.1",
    "express": "4.18.2",
    "gunzip-maybe": "1.4.2",
    "highlight.js": "11.7.0",
    "jsesc": "3.0.2",
    "lru-cache": "7.17.0",
    "mime": "3.0.0",
    "morgan": "1.10.0",
    "semver": "7.3.8",
    "sri-toolbox": "0.2.0",
    "tar-stream": "3.0.0",
    "tempy": "3.0.0",
    "validate-npm-package-name": "5.0.0",
    "warning": "4.0.3",
    "whatwg-url": "12.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-export-default-from": "7.18.6",
    "@babel/plugin-syntax-export-namespace-from": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/preset-env": "7.20.2",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "29.4.3",
    "dotenv": "16.0.3",
    "eslint": "8.34.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "git-rev-sync": "3.0.2",
    "jest": "29.4.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "rollup": "2.79.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-url": "3.0.1",
    "supertest": "6.3.3"
  },
  "engines": {
    "node": "16.17.0"
  }
}