{
  "name": "@sab/cdn",
  "private": true,
  "description": "A CDN service for frontend artifacts within the SAB infrastructure",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "files": [
    ".env.example",
    "dist",
    "Dockerfile",
    "docker-compose.yml"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "ts-node --transpileOnly --logError ./esbuild.config.ts",
    "start": "nodemon",
    "start:docker": "docker compose up --build"
  },
  "dependencies": {
    "@babel/core": "7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/types": "7.21.3",
    "cheerio": "1.0.0-rc.12",
    "cors": "2.8.5",
    "etag": "1.8.1",
    "express": "4.18.2",
    "gunzip-maybe": "1.4.2",
    "lru-cache": "7.17.0",
    "mime": "3.0.0",
    "morgan": "1.10.0",
    "semver": "7.3.8",
    "sri-toolbox": "0.2.0",
    "tar-stream": "3.0.0",
    "validate-npm-package-name": "5.0.0",
    "warning": "4.0.3",
    "whatwg-url": "12.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/etag": "1.8.1",
    "@types/express": "4.17.17",
    "@types/gunzip-maybe": "1.4.0",
    "@types/jest": "29.4.0",
    "@types/morgan": "1.9.4",
    "@types/node": "18.14.2",
    "@types/supertest": "2.0.12",
    "@types/tar-stream": "2.2.2",
    "@types/validate-npm-package-name": "4.0.0",
    "@types/warning": "3.0.0",
    "@types/whatwg-url": "11.0.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "dotenv": "16.0.3",
    "esbuild": "0.17.10",
    "esbuild-copy-static-files": "0.1.0",
    "esbuild-plugin-clean": "1.0.1",
    "eslint": "8.35.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "git-rev-sync": "3.0.2",
    "jest": "29.4.3",
    "nodemon": "2.0.20",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "18.15.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/src/__mocks__/style.mock.ts",
      "\\.png$": "<rootDir>/src/__mocks__/image.mock.ts",
      "getStats": "<rootDir>/src/__mocks__/get-stats.mock.ts",
      "utils\\/npm": "<rootDir>/src/__mocks__/npm.mock.ts"
    },
    "preset": "ts-jest",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  },
  "version": "0.0.1"
}
